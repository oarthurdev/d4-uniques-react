{
    "version": 2,
    "builds": [
      { "src": "app.js", "use": "@vercel/node" },
      { "src": "client/build/**/*", "use": "@vercel/static-build" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1" },
      { "src": "/", "dest": "/app.js" },
      { "src": "/(.*)", "dest": "/client/build/$1" }
    ]
  }
  